#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    const int MAX_SIZE = 15;
    int arr[MAX_SIZE];
    int count = 0;
    int num;

    // Input values
    while (count < MAX_SIZE) {
        cin >> num;

        if (num == 0)
            break;

        arr[count] = num;
        count++;
    }

    // If no numbers entered
    if (count == 0) {
        cout << "Invalid";
        return 0;
    }

    // Bubble Sort
    for (int i = 0; i < count - 1; i++) {
        for (int j = 0; j < count - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    double median;

    if (count % 2 == 1) {   // Odd
        median = arr[count / 2];
    } else {               // Even
        median = (arr[count / 2 - 1] + arr[count / 2]) / 2.0;
    }

    cout << fixed << setprecision(1) << median;

    return 0;
}
